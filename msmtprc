
account gmail
host smtp.gmail.com
user rajsahae@gmail.com
passwordeval gpg --no-tty --quiet --decrypt ~/.msmtp-gmail.gpg
from rajsahae@gmail.com
auth on
tls on
tls_starttls off
tls_trust_file ~/.mutt/Equifax_Secure_CA.cert


account tesla
host smtp-gw.teslamotors.com
user rsahae@teslamotors.com
passwordeval gpg --no-tty --quiet --decrypt ~/.msmtp-tesla.gpg
from rsahae@teslamotors.com
auth off


account default : gmail
